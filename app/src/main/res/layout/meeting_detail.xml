<?xml version="1.0" encoding="utf-8"?>
<layout>

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/bg_ash"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/action_bar_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/green"

            android:elevation="@dimen/_1sdp">


            <RelativeLayout
                android:id="@+id/act"
                android:layout_width="match_parent"
                android:layout_height="?actionBarSize"
                android:elevation="@dimen/_2sdp">

                <RelativeLayout

                    android:id="@+id/backArraow"
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/cart"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:src="@mipmap/back_arrow_white"
                        android:tint="@color/colorPrimary" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/backArraow">

                    <com.zeyalychat.com.utils.CustomSemiBoldTextview
                        android:id="@+id/title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:layout_marginRight="@dimen/_10sdp"

                        android:background="@android:color/transparent"
                        android:text="@string/Meeting"
                        android:textColor="@color/colorPrimary"
                        android:textColorHint="@color/colorPrimary"
                        android:textSize="@dimen/_12sdp" />
                </RelativeLayout>


            </RelativeLayout>

            <View
                android:id="@+id/v1"
                android:layout_width="wrap_content"
                android:layout_height="1sp"
                android:layout_below="@+id/act"
                android:background="@color/receiver">

            </View>


        </RelativeLayout>

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_above="@+id/bottom_layout"
            android:layout_below="@+id/action_bar_layout"
            android:visibility="visible">

            <LinearLayout
                android:id="@+id/home_L"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/action_bar_layout"
                android:orientation="vertical"
                android:visibility="visible">


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_8sdp"
                    android:background="@drawable/background_box">

                    <RelativeLayout
                        android:id="@+id/view_foreground"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:layout_marginBottom="@dimen/_5sdp">


                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_marginRight="@dimen/_5sdp"
                            android:layout_marginBottom="@dimen/_5sdp"
                            android:visibility="visible">


                            <RelativeLayout
                                android:id="@+id/row_rl"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/_5sdp"

                                android:layout_marginBottom="@dimen/_2sdp">


                                <RelativeLayout
                                    android:id="@+id/TitleL"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">

                                    <RelativeLayout
                                        android:id="@+id/titleLayout"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content">

                                        <com.zeyalychat.com.utils.CustomSemiBoldTextview
                                            android:id="@+id/datel"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="12-7-2021"
                                            android:textColor="@color/full_textt_color"
                                            android:textSize="@dimen/_11sdp"
                                            android:visibility="gone" />

                                        <com.zeyalychat.com.utils.CustomSemiBoldTextview
                                            android:id="@+id/Title"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"

                                            android:layout_toRightOf="@+id/datel"
                                            android:text="12-7-2021"
                                            android:textColor="@color/full_textt_color"
                                            android:textSize="@dimen/_12sdp"
                                            android:visibility="visible" />
                                    </RelativeLayout>

                                    <RelativeLayout
                                        android:id="@+id/examlayout"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentRight="true"
                                        android:visibility="gone">


                                        <ImageView
                                            android:id="@+id/clock"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_centerVertical="true"
                                            android:src="@mipmap/clock"
                                            android:tint="@color/medium_text_color">

                                        </ImageView>


                                        <com.zeyalychat.com.utils.CustomSemiBoldTextview
                                            android:id="@+id/time"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginLeft="@dimen/_3sdp"
                                            android:layout_marginRight="@dimen/_5sdp"


                                            android:layout_toRightOf="@+id/clock"
                                            android:gravity="end"

                                            android:text="10:00 am - 11:45 am"
                                            android:textColor="@color/medium_text_color"
                                            android:textSize="@dimen/_10sdp" />


                                    </RelativeLayout>
                                </RelativeLayout>

                                <RelativeLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/TitleL"
                                    android:layout_marginTop="@dimen/_5sdp">

                                    <RelativeLayout
                                        android:id="@+id/lay"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content">

                                        <com.zeyalychat.com.utils.CustomSemiBoldTextview
                                            android:id="@+id/starttime"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="Start Time:"
                                            android:textColor="@color/medium_text_color"
                                            android:textSize="@dimen/_10sdp"
                                            android:tint="@color/medium_text_color">

                                        </com.zeyalychat.com.utils.CustomSemiBoldTextview>

                                        <com.zeyalychat.com.utils.CustomSemiBoldTextview
                                            android:id="@+id/starttimeTxt"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginLeft="@dimen/_5sdp"


                                            android:layout_toRightOf="@+id/starttime"

                                            android:text="200"
                                            android:textColor="@color/medium_text_color"
                                            android:textSize="@dimen/_10sdp" />
                                    </RelativeLayout>

                                    <RelativeLayout
                                        android:id="@+id/roomlayout"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentRight="true">


                                        <com.zeyalychat.com.utils.CustomSemiBoldTextview
                                            android:id="@+id/endtime"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="End Time:"
                                            android:textColor="@color/medium_text_color"
                                            android:textSize="@dimen/_10sdp"
                                            android:tint="@color/medium_text_color">

                                        </com.zeyalychat.com.utils.CustomSemiBoldTextview>


                                        <com.zeyalychat.com.utils.CustomSemiBoldTextview
                                            android:id="@+id/endtimeTxt"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginLeft="@dimen/_3sdp"
                                            android:layout_marginRight="@dimen/_5sdp"


                                            android:layout_toRightOf="@+id/endtime"
                                            android:gravity="end"

                                            android:text="8"
                                            android:textColor="@color/medium_text_color"
                                            android:textSize="@dimen/_10sdp" />


                                    </RelativeLayout>
                                </RelativeLayout>

                            </RelativeLayout>


                        </RelativeLayout>


                    </RelativeLayout>


                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_8sdp"
                    android:layout_marginRight="@dimen/_8sdp"
                    android:layout_marginBottom="@dimen/_8sdp"
                   >

                    <RelativeLayout
                        android:id="@+id/meetingurlR"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">


                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"


                            android:visibility="visible">


                            <RelativeLayout

                                android:id="@+id/meetingurlRL"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">


                                <RelativeLayout
                                    android:id="@+id/meetingURLR"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_5sdp"

                                    android:background="@drawable/background_box">

                                    <RelativeLayout
                                        android:id="@+id/mURLR"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="@dimen/_10sdp"
                                        android:layout_marginTop="@dimen/_5sdp"
                                        android:layout_marginRight="@dimen/_5sdp"
                                        android:layout_marginBottom="@dimen/_5sdp">

                                        <com.zeyalychat.com.utils.CustomSemiBoldTextview
                                            android:id="@+id/MeetingURLLBL"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="@string/Meeting_Url"
                                            android:textColor="@color/full_textt_color"
                                            android:textSize="@dimen/_11sdp" />

                                        <com.zeyalychat.com.utils.CustomSemiBoldTextview
                                            android:id="@+id/MeetingUrlTxt"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_below="@+id/MeetingURLLBL"

                                            android:layout_marginTop="@dimen/_2sdp"
                                            android:text="zeyaly.com"
                                            android:textColor="@color/medium_text_color"
                                            android:textSize="@dimen/_11sdp"
                                            android:visibility="visible" />
                                    </RelativeLayout>


                                </RelativeLayout>

                                <RelativeLayout
                                    android:id="@+id/passcodeRL"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/meetingURLR"
                                    android:layout_marginBottom="@dimen/_5sdp"
                                    android:background="@drawable/background_box">

                                    <RelativeLayout
                                        android:id="@+id/passcodeR"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="@dimen/_10sdp"
                                        android:layout_marginTop="@dimen/_5sdp"
                                        android:layout_marginRight="@dimen/_5sdp"
                                        android:layout_marginBottom="@dimen/_5sdp">

                                        <com.zeyalychat.com.utils.CustomSemiBoldTextview
                                            android:id="@+id/PassCodeLBL"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="@string/passcode"
                                            android:textColor="@color/full_textt_color"
                                            android:textSize="@dimen/_12sdp"
                                            android:tint="@color/medium_text_color">

                                        </com.zeyalychat.com.utils.CustomSemiBoldTextview>

                                        <com.zeyalychat.com.utils.CustomSemiBoldTextview
                                            android:id="@+id/passcodeTxt"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_below="@+id/PassCodeLBL"


                                            android:layout_marginTop="@dimen/_2sdp"

                                            android:text="200"
                                            android:textColor="@color/medium_text_color"
                                            android:textSize="@dimen/_11sdp" />
                                    </RelativeLayout>


                                </RelativeLayout>

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@+id/passcodeRL"

                                    android:background="@drawable/background_box">

                                    <RelativeLayout
                                        android:id="@+id/MeetingIDR"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="@dimen/_10sdp"
                                        android:layout_marginTop="@dimen/_5sdp"
                                        android:layout_marginRight="@dimen/_5sdp"
                                        android:layout_marginBottom="@dimen/_5sdp">

                                        <com.zeyalychat.com.utils.CustomSemiBoldTextview
                                            android:id="@+id/MeetingIDLBL"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="@string/Meeting_id"
                                            android:textColor="@color/full_textt_color"
                                            android:textSize="@dimen/_12sdp"
                                            android:tint="@color/medium_text_color">

                                        </com.zeyalychat.com.utils.CustomSemiBoldTextview>

                                        <com.zeyalychat.com.utils.CustomSemiBoldTextview
                                            android:id="@+id/MeetingIDtxt"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_below="@+id/MeetingIDLBL"


                                            android:layout_marginTop="@dimen/_2sdp"

                                            android:text="200"
                                            android:textColor="@color/medium_text_color"
                                            android:textSize="@dimen/_11sdp" />
                                    </RelativeLayout>


                                </RelativeLayout>


                            </RelativeLayout>


                        </RelativeLayout>


                    </RelativeLayout>


                </RelativeLayout>

            </LinearLayout>
        </androidx.core.widget.NestedScrollView>


        <RelativeLayout
            android:id="@+id/bottom_layout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="@dimen/_10sdp"

            android:visibility="gone">


            <com.zeyalychat.com.utils.CustomButtonBold
                android:id="@+id/submit"
                android:layout_width="@dimen/_100sdp"
                android:layout_height="@dimen/_40sdp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/_20sdp"
                android:layout_marginTop="@dimen/_20sdp"
                android:layout_marginRight="@dimen/_20sdp"
                android:background="@drawable/button_login"
                android:elevation="@dimen/_2sdp"
                android:outlineAmbientShadowColor="@color/green"
                android:outlineSpotShadowColor="@color/green"
                android:text="@string/Submit"
                android:textAllCaps="false"
                android:textColor="@color/white"
                android:textSize="@dimen/_12sdp" />


        </RelativeLayout>
    </RelativeLayout>
</layout>
