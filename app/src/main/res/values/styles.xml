<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>

    <style name="NoActionBar_Theme" parent="Theme.AppCompat.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/dark_green</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <!--<item name="android:windowTranslucentStatus">true</item>-->
    </style>
    <style name="CustomTextAppearance" parent="TextAppearance.AppCompat.Medium">
        <item name="android:textColor">@color/full_textt_color</item>
        <item name="android:textSize">@dimen/_10sdp</item>
        <item name="fontFamily">@font/googlesansmedium</item>
    </style>
    <style name="TextAppearance" parent="TextAppearance.AppCompat.Medium">
        <item name="android:textColor">@color/medium_text_color</item>
        <item name="android:textSize">@dimen/_10sdp</item>
        <item name="fontFamily">@font/googlesansmedium</item>

      <!--  <item name="fontFamily">@font/GoogleSans-Medium</item>-->
    </style>






</resources>
